  "scripts": {
    "start": "expo start",
    "start:clean": "bash scripts/kill-expo.sh && expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "ios:build:production": "eas build --platform ios --profile production --non-interactive",
    "ios:submit:production": "eas submit --platform ios --profile production --non-interactive",
    "ios:testflight": "eas build --platform ios --profile production-widgets --non-interactive --auto-submit",
    "ios:testflight:no-widgets": "eas build --platform ios --profile production --non-interactive --auto-submit",
    "lint": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "visual:collect": "node scripts/visual-regression/collect.js",
    "visual:compare": "node scripts/visual-regression/compare.js",
    "visual:update": "node scripts/visual-regression/compare.js --update"
  },
