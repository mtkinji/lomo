{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "KWILT_ENABLE_APP_GROUPS": "0",
        "KWILT_ENABLE_WIDGETS": "0",
        "SUPABASE_URL": "https://auth.kwilt.app",
        "AI_PROXY_BASE_URL": "https://auth.kwilt.app/functions/v1/ai-chat"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "KWILT_ENABLE_APP_GROUPS": "0",
        "KWILT_ENABLE_WIDGETS": "0",
        "SUPABASE_URL": "https://auth.kwilt.app",
        "AI_PROXY_BASE_URL": "https://auth.kwilt.app/functions/v1/ai-chat"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "KWILT_ENABLE_APP_GROUPS": "0",
        "KWILT_ENABLE_WIDGETS": "0",
        "SUPABASE_URL": "https://auth.kwilt.app",
        "AI_PROXY_BASE_URL": "https://auth.kwilt.app/functions/v1/ai-chat",
        "POSTHOG_HOST": "https://us.i.posthog.com",
        "KWILT_SVG_SHIM": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6755990439"
      }
    }
  }
}


